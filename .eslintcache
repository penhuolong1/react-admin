.jsx'\r\nimport Layout from '@/views/layout/index.jsx'\r\nimport { getUserInfo } from '@/store/actions/user'\r\n\r\nclass Routers extends Component {\r\n  componentDidMount() {\r\n  }\r\n  render() {\r\n    const { token, userInfo, getUserInfo } = this.props;\r\n    return (\r\n      <BrowserRouter>\r\n        <Switch>\r\n          <Route exact path=\"/login\" component={Login}></Route>\r\n          {/* <Route exact path=\"/dashboard\" component={Layout}></Route> */}\r\n          <Route\r\n            path=\"/\"\r\n            render={() => {\r\n              if (!token) {\r\n                return <Redirect to=\"/login\" />;\r\n              } else {\r\n                if (userInfo.role) {\r\n                  return <Layout />;\r\n                } else {\r\n                  getUserInfo(token).then(() => <Layout />);\r\n                }\r\n              }\r\n            }}\r\n          />\r\n        </Switch>\r\n      </BrowserRouter>\r\n    );\r\n  }\r\n}\r\n\r\nexport default connect(state => state.user, {getUserInfo})(Routers);\r\n","D:\\react\\react-admin\\src\\mock\\index.js",[],"D:\\react\\react-admin\\src\\utils\\constVal.js",[],"D:\\react\\react-admin\\src\\views\\layout\\index.jsx",[],"D:\\react\\react-admin\\src\\views\\login\\index.jsx",[],"D:\\react\\react-admin\\src\\mock\\login.js",[],"D:\\react\\react-admin\\src\\store\\actions\\user.js",[],"D:\\react\\react-admin\\src\\store\\index.js",[],"D:\\react\\react-admin\\src\\views\\layout\\header\\index.jsx",["245","246","247"],"import React, { Component } from 'react';\r\nimport * as types from \"@/store/actions-type\"\r\nimport { Layout, Menu, Dropdown, Avatar, Button, message } from 'antd';\r\nimport { Link } from \"react-router-dom\";\r\nimport { connect } from 'react-redux'\r\nimport { logout } from '@/store/actions/user.js'\r\nimport { toggleCollapsed } from '@/store/actions/menu.js'\r\nimport screenfull from 'screenfull'\r\nimport Breadcrumb from './breadcrumb'\r\n\r\nimport './index.scss'\r\nimp